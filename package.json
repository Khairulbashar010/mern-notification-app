{
  "name": "react-express",
  "version": "1.0.0",
  "description": "for interview",
  "main": "server.js",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix webapp && npm run build --prefix webapp",
    "start": "node server.js",
    "server": "nodemon server.js",
    "webapp": "npm start --prefix webapp",
    "dev": "concurrently \"npm run server\" \"npm run webapp\""
  },
  "keywords": [
    "node",
    "express",
    "react",
    "javascript"
  ],
  "author": "Khairul Bashar",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "concurrently": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.6.0",
    "mongoose": "^5.12.2"
  },
  "devDependencies": {
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7"
  }
}
